<?xml version="1.0" encoding="utf-8" ?>
<equations>
  
  <equation>
    <name>Average Velocity</name>
    <path>Physics/Mechanics/Average Velocity</path>
    <info>The average velocity over a time period.</info>
    <eq>v=s/t</eq>
    <var unit="\frac{m}{s}" hint="Velocity">
      v
    </var>
    <var unit="m" hint="Displacement">
      s
    </var>
    <var unit="s" hint="Time">
      t
    </var>
  </equation>
  
</equations>
